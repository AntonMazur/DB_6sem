<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.LogOutWindow"
            prefHeight="260.0" prefWidth="550.0">
    <ImageView fx:id="background" fitWidth="550.0" fitHeight="260.0"/>
    <Label layoutX="130" layoutY="30" text="Вы уверены, что хотите выйти?"/>
    <Button fx:id="cancellogOut" layoutY="180" layoutX="100" prefWidth="100" text="Нет" onAction="#close"/>
    <Button fx:id="logOut" layoutY="180" layoutX="400" prefWidth="100" text="Да" onAction="#logOut"/>

    <stylesheets>
        <URL value="@/css/logOutWindow.css" />
    </stylesheets>

</AnchorPane>
