<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller"
            prefHeight="400.0" prefWidth="600.0">

    <Label layoutX="24.0" layoutY="14.0" text="Login:" />
    <Label layoutX="24.0" layoutY="63.0" text="Password:" />
    <TextField fx:id="loginField" layoutX="24.0" layoutY="30.0" prefWidth="200.0" />
    <PasswordField fx:id="pwField" layoutX="24.0" layoutY="86.0" prefWidth="200.0" />
    <Button fx:id="logBut" layoutX="100" layoutY="140" text="Log in" onAction="#tryLogIn"/>

</AnchorPane>
